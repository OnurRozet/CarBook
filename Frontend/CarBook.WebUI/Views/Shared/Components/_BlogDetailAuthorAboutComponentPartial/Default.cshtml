@model List<CarBook.Application.Features.Mediator.Results.BlogResults.GetBlogWithAuthorIdQueryResult>
<div class="about-author d-flex p-4 bg-light">
    @foreach (var item in Model)
    {
          <div class="bio mr-5">
        <img src="@item.AuthorImage" alt="Image placeholder" class="img-fluid mb-3">
    </div>
    <div class="desc">
        <h3>@item.AuthorName</h3>
        <p>@item.AuthorDescription</p>
    </div>
    }
  
</div>
